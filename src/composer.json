{
  "name": "woocart-defaults",
  "description": "Manage and deploy WordPress WooCommerce configuration changes",
  "keywords": [
    "WordPress",
    "plugin"
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.2.0",
    "symfony/yaml": "^3.3",
    "ext-json": "*",
    "ext-gd": "*",
    "krakjoe/pthreads-polyfill": "^1.1",
    "lcobucci/jwt": "^3.2",
    "predis/predis": "^1.1",
    "ext-zend-opcache": "*"
  },
  "autoload": {
    "psr-4": {
      "Niteo\\WooCart\\Defaults\\": "classes",
      "Niteo\\WooCart\\Defaults\\Importers\\": "importers"
    }
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.0"
  }
}
